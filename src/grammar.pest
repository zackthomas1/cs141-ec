WHITESPACE = _{ " " | "\t" | "\r" | "\n" }
COMMENT = _{ ";;" ~ (!NEWLINE ~ ANY)* }

number = @{ "-"? ~ ASCII_DIGIT+ }
symbol = @{ (ASCII_ALPHANUMERIC | "_" | "+" | "-" | "*" | "/" | "\\" | "=" | "<" | ">" | "!" | "&")+ }
string = @{ "\"" ~ (!"\"" ~ ANY)* ~ "\"" }
sexpr = { "(" ~ expr* ~ ")" }
qexpr = { "'" ~ expr }
expr = { number | symbol | string | sexpr | qexpr }
lispy = { SOI ~ expr* ~ EOI }
